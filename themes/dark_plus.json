{
  "$schema": "vscode://schemas/color-theme",
  "name": "Dark+",
  "include": "./dark_vs.json",
  "tokenColors": [
    {
      "name": "Function declarations",
      "scope": [
        "entity.name.function",
        "support.function",
        "support.constant.handlebars",
        "source.powershell variable.other.member",
        "entity.name.operator.custom-literal"
      ],
      "settings": {
        "foreground": "#50FA7B"
      }
    },
    {
      "name": "Types declaration and references",
      "scope": [
        "support.class",
        "support.type",
        "entity.name.type",
        "entity.name.namespace",
        "entity.other.attribute",
        "entity.name.scope-resolution",
        "entity.name.class",
        "storage.type.numeric.go",
        "storage.type.byte.go",
        "storage.type.boolean.go",
        "storage.type.string.go",
        "storage.type.uintptr.go",
        "storage.type.error.go",
        "storage.type.rune.go",
        "storage.type.cs",
        "storage.type.generic.cs",
        "storage.type.modifier.cs",
        "storage.type.variable.cs",
        "storage.type.annotation.java",
        "storage.type.generic.java",
        "storage.type.java",
        "storage.type.object.array.java",
        "storage.type.primitive.array.java",
        "storage.type.primitive.java",
        "storage.type.token.java",
        "storage.type.groovy",
        "storage.type.annotation.groovy",
        "storage.type.parameters.groovy",
        "storage.type.generic.groovy",
        "storage.type.object.array.groovy",
        "storage.type.primitive.array.groovy",
        "storage.type.primitive.groovy"
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Types declaration and references, TS grammar specific",
      "scope": [
        "meta.type.cast.expr",
        "meta.type.new.expr",
        "support.constant.math",
        "support.constant.dom",
        "support.constant.json",
        "entity.other.inherited-class",
        "meta.function-call.generic.python"
      ],
      "settings": {
        "foreground": "#50FA7B"
      }
    },
    {
      "name": "Control flow / Special keywords",
      "scope": [
        "keyword.control",
        "source.cpp keyword.operator.new",
        "keyword.operator.delete",
        "keyword.other.using",
        "keyword.other.operator",
        "entity.name.operator"
      ],
      "settings": {
        "foreground": "#ff79c6"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        // "variable",
        "meta.definition.variable.name",
        "entity.name.variable",
      ],
      "settings": {
        "foreground": "#d6eff5"
      }
    },
    {
      "name": "Variable and parameter name",
      "scope": [
        "support.variable",
      ],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "scope": "constant.other.placeholder",
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Constants and enums",
      "scope": [
        "variable.other.constant",
      ],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "scope": "variable.other.enummember",
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Object keys, TS grammar specific",
      "scope": [
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Constant escape sequences",
      "scope": [
          "constant.character.escape",
          "constant.character.string.escape",
          "constant.regexp"
      ],
      "settings": {
          "foreground": "#FF79C6"
      }
  },
    {
      "name": "CSS property value",
      "scope": [
        "support.constant.property-value",
        "support.constant.font-name",
        "support.constant.media-type",
        "support.constant.media",
        "constant.other.color.rgb-value",
        "constant.other.rgb-value",
        "support.constant.color"
      ],
      "settings": {
        "foreground": "#F8F8F2"
      }
    },
    {
      "scope": [
        "constant.character",
        "constant.other.option"
      ],
      "settings": {
        "foreground": "#FF79C6" 
      }
    },
    {
      "name": "RegExp string",
      "scope": [
          "string.regexp",
      ],
      "settings": {
          "foreground": "#F1FA8C"
      }
  },
  {
    "scope": ["constant.other.character-class.set.regexp",
    "constant.character.escape.backslash.regexp"],
    "settings": {
      "foreground": "#FF79C6"
    }
  },
  {
    "name": "Capture groups",
    "scope": [
        "punctuation.definition.group.regexp"
    ],
    "settings": {
        "foreground": "#FFB86C"
    }
},
{
    "name": "Assertion operators",
    "scope": [
        "punctuation.definition.group.assertion.regexp",
        "keyword.operator.negation.regexp"
    ],
    "settings": {
        "foreground": "#FF5555"
    }
},
{
    "name": "Positive lookaheads",
    "scope": [
        "meta.assertion.look-ahead.regexp"
    ],
    "settings": {
        "foreground": "#50FA7B"
    }
},
  {
    "name": "Character group",
    "scope": [
        "punctuation.definition.character-class.regexp"
    ],
    "settings": {
        "foreground": "#8BE9FD"
    }
},
  {
    "name": "Non-capture operators",
    "scope": [
        "punctuation.definition.group.capture.regexp"
    ],
    "settings": {
        "foreground": "#FF79C6"
    }
},
    {
      "scope": "entity.name.label",
      "settings": {
        "foreground": "#f8f8f2"
      }
    },
    {
      "scope": "variable.other.readwrite.js meta.block.js meta.arrow.js",
      "settings": {
        "foreground": "#3c00ff"
      }
    },
    {
      "scope": "variable.parameter.js",
      "settings": {
        "foreground": "#FFB86C"
      }
    }
  ],
  "semanticTokenColors": {
    // variable.parameter.function-call.python
    "parameter:javascript": "#FFB86C",
    "variable:python": "#d6eff5",
    "parameter:python":"#FFB86C",
    "newOperator": "#ff79c6",
    "stringLiteral": "#F8F8F2",
    "customLiteral": "#DCDCAA",
    "numberLiteral": "#b5cea8"
  }
}